<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="150.0" prefWidth="346.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1"
      xmlns="http://javafx.com/javafx/17" fx:controller="hu.petrik.museumfrontendjavafx.general.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label text="Üdvözlöm a múzeumunk főoldalán"/>
    <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
        <children>
            <Button onAction="#toPaintings" mnemonicParsing="false" text="Festmények oldal">
                <HBox.margin>
                    <Insets right="20.0"/>
                </HBox.margin>
            </Button>
            <Button onAction="#toStatues" text="Szobrok oldal">
                <HBox.margin>
                    <Insets left="20.0"/>
                </HBox.margin>
            </Button>
        </children>
    </HBox>
</VBox>
